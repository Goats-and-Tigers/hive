#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ashtyn/code/gt-engine/www/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/bin/node_modules:/home/ashtyn/code/gt-engine/www/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/node_modules:/home/ashtyn/code/gt-engine/www/node_modules/.pnpm/typescript@4.6.4/node_modules:/home/ashtyn/code/gt-engine/www/node_modules/.pnpm/node_modules:/home/ashtyn/code/gt-engine/www/node_modules:/home/ashtyn/code/gt-engine/node_modules:/home/ashtyn/code/node_modules:/home/ashtyn/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ashtyn/code/gt-engine/www/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/bin/node_modules:/home/ashtyn/code/gt-engine/www/node_modules/.pnpm/typescript@4.6.4/node_modules/typescript/node_modules:/home/ashtyn/code/gt-engine/www/node_modules/.pnpm/typescript@4.6.4/node_modules:/home/ashtyn/code/gt-engine/www/node_modules/.pnpm/node_modules:/home/ashtyn/code/gt-engine/www/node_modules:/home/ashtyn/code/gt-engine/node_modules:/home/ashtyn/code/node_modules:/home/ashtyn/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsserver" "$@"
else
  exec node  "$basedir/../../bin/tsserver" "$@"
fi
